<h3>Create a new bookmark here</h3>
<div class="container">
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.button :submit, class:"btn btn-outline-success"%>
  <% end %>
  <br><h3><%= link_to "Back to index", lists_path, class:"btn btn-primary text-decoration-none" %></h3>
</div>
